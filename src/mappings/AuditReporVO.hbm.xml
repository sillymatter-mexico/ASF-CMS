<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="asf.cms.model" assembly="cmsDemo">

  <class name="asf.cms.model.AuditReportVO" table="audit_report">

    <id name="Id">
      <column name="id" sql-type="int(11)" not-null="true"/>
      <generator class="identity" />
    </id>
    <property name="Year">
      <column name="year"/>
    </property>
    <property name="Type">
      <column name="type"/>
    </property>
    <property name="DirectoryPath">
      <column name="directory_path"/>
    </property>
    <property name="Published">
      <column name="published"/>
    </property>
    <property name="PublicationPermalink">
      <column name="publication_permalink"/>
    </property>
    <property name="MainFilePath">
      <column name="main_file_path"/>
    </property>
    <property name="PresentationPath">
      <column name="presentation_path"/>
    </property>
    <property name="ExecutiveReportPath">
      <column name="executive_report_path"/>
    </property>
  </class>
</hibernate-mapping>