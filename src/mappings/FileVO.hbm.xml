<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="asf.cms.model" assembly="cmsDemo">

  <class name="asf.cms.model.FileVO" table="file">

    <id name="Id">
      <column name="id" sql-type="int(11)" not-null="true"/>
      <generator class="identity" />
    </id>
    <property name="PublicationId">
      <column name="publication_id"/>
    </property>
    <property name="Mime">
      <column name="mime"/>
    </property>
    <property name="Path">
      <column name="path"/>
    </property>
    <property name="Name">
      <column name="name"/>
    </property>
    <property name="Title">
      <column name="Title"/>
    </property>
    <property name="IsMain">
      <column name="is_main"/>
    </property>
  </class>
</hibernate-mapping>