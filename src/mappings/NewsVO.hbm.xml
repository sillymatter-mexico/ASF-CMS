<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="asf.cms.model" assembly="cmsDemo">

  <class name="asf.cms.model.NewsVO" table="news">

    <id name="Id">
      <column name="id" sql-type="int(11)" not-null="true"/>
    </id>
    
    <property name="Title">
      <column name="title"/>
    </property>

    <property name="Permalink">
      <column name="permalink"/>
    </property>

    <property name="Updated">
      <column name="updated"/>
    </property>

    <property name="NewsPin">
      <column name="news_pin"/>
    </property>

    <property name="NewsTTL">
      <column name="news_ttl"/>
    </property>

    <property name="Content">
      <column name="content"/>
    </property>
    
    <property name="LanguageId">
      <column name="language_id"/>
    </property>
    
  </class>
</hibernate-mapping>